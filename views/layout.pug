html
  head
    title #{title} 
    meta(charset="utf-8")
    meta(name="viewport", content="width=device-width, initial-scale=1, shrink-to-fit=no")
    //- link(rel='stylesheet', href='https://cdn.leafletjs.com/leaflet/v0.7.7/leaflet.css')
    link(rel='stylesheet', href="https://code.getmdl.io/1.3.0/material.teal-cyan.min.css")
    script(src="/css/material.min.js")
    //- script(src='https://cdn.leafletjs.com/leaflet/v0.7.7/leaflet.js')
    //- link(rel="stylesheet" href="/style.css")
  body 
    div.mdl-layout.mdl-js-layout.mdl-layout--fixed-header
      header.mdl-layout__header
        div.mdl-layout__header-row
          div.mdl-layout-spacer
          nav.mdl-navigation 
            a.mdl-navigation__link(href="/") home
            a.mdl-navigation__link(href="/geo") all notes..'/geo'
            a.mdl-navigation__link(href="") Link 3
      block content